Algoritmo "temperatura"

Var

   a, b, c : real

Inicio

   escreval("Digite tres numeros: ")
   leia(a)
   leia(b)
   leia(c)
   
   se (a > b) e (a > c) entao
   escreva("MAIOR = ", a:4:2)
   senao
   se (b > a) e (b > c) entao
   escreva("MAIOR = ", b:4:2)
   senao
   escreva("MAIOR = ", c:4:2)
   fimse
   fimse



Fimalgoritmo